{
  "additionalReviewers": ["bchew", "xiaofan2406"],
  "branchPrefix": "renovate-",
  "extends": ["config:base", ":disableDigestUpdates", ":onlyNpm"],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchPackageNames": ["node", "npm"],
      "enabled": false
    },
    {
      "matchPackagePatterns": ["eslint"],
      "groupName": "eslint",
      "recreateClosed": false
    },
    {
      "matchPackagePatterns": ["redux"],
      "groupName": "redux",
      "recreateClosed": false
    },
    {
      "matchPackagePatterns": ["webpack"],
      "groupName": "webpack",
      "recreateClosed": false
    },
    {
      "matchPackagePatterns": ["rollup"],
      "groupName": "rollup",
      "recreateClosed": false
    },
    {
      "matchPackagePatterns": ["postcss"],
      "groupName": "postcss",
      "recreateClosed": false
    },
    {
      "matchPackagePatterns": ["classnames"],
      "groupName": "classnames",
      "recreateClosed": false
    },
    {
      "matchPackagePatterns": ["lodash"],
      "groupName": "lodash",
      "recreateClosed": false
    },
    {
      "matchPackagePatterns": ["jest"],
      "groupName": "jest",
      "recreateClosed": false
    },
    {
      "matchPackagePatterns": ["draft-js"],
      "groupName": "draft-js",
      "recreateClosed": false
    },
    {
      "matchPackagePatterns": ["react-datepicker"],
      "groupName": "react-datepicker",
      "recreateClosed": false
    },
    {
      "matchPackagePatterns": ["react-select"],
      "groupName": "react-select",
      "recreateClosed": false
    },
    {
      "matchPackagePatterns": ["react-slick"],
      "groupName": "react-slick",
      "recreateClosed": false
    },
    {
      "matchPackagePatterns": ["@testing-library"],
      "groupName": "testing-library",
      "recreateClosed": false
    },
    {
      "matchPackagePatterns": ["pino"],
      "groupName": "pino",
      "recreateClosed": false
    },
    {
      "matchPackagePatterns": ["@types", "typescript"],
      "groupName": "typescript",
      "recreateClosed": false
    }
  ],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "rangeStrategy": "bump",
  "rollbackPrs": true,
  "schedule": [
    "never"
  ],
  "timezone": "Australia/Melbourne",
  "vulnerabilityAlerts": {
    "enabled": true,
    "rangeStrategy": "bump"
  }
}
