{
  "additionalReviewers": [
    "bchew",
    "xiaofan2406"
  ],
  "branchPrefix": "renovate-",
  "constraints": {
    "npm": "10.8.2"
  },
  "extends": [
    "config:recommended",
    "docker:disable",
    ":disableDigestUpdates",
    ":npm",
    ":semanticCommits"
  ],
  "labels": [
    "dependencies"
  ],
  "minimumReleaseAge": "3 days",
  "packageRules": [
    {
      "matchPackageNames": [
        "node",
        "npm"
      ],
      "enabled": false
    },
    {
      "matchPackageNames": [
        "axios-mock-adapter",
        "@commitlint/**",
        "@cucumber/cucumber",
        "c8",
        "deep-equal-in-any-order",
        "husky",
        "lint-staged",
        "loglevel",
        "mocha",
        "nodemon",
        "nyc",
        "prettier",
        "sinon",
        "stylelint*",
        "supertest"
      ],
      "groupName": "devDeps"
    },
    {
      "matchPackageNames": [
        "@axe-core/**",
        "@wdio/**",
        "allure-commandline",
        "axe-html-reporter",
        "otpauth",
        "tough-cookie",
        "webdriverio"
      ],
      "groupName": "e2e"
    },
    {
      "matchPackageNames": [
        "async*"
      ],
      "groupName": "async"
    },
    {
      "matchPackageNames": [
        "axios*"
      ],
      "groupName": "axios"
    },
    {
      "matchPackageNames": [
        "dd-trace",
        "@datadog/**"
      ],
      "groupName": "datadog"
    },
    {
      "matchPackageNames": [
        "puppeteer",
        "sharp"
      ],
      "groupName": "docs"
    },
    {
      "matchPackageNames": [
        "draft-js",
        "@draft-js/**"
      ],
      "groupName": "draft-js"
    },
    {
      "matchPackageNames": [
        "eslint*"
      ],
      "groupName": "eslint"
    },
    {
      "matchPackageNames": [
        "body-parser",
        "express*",
        "serve-static"
      ],
      "groupName": "express"
    },
    {
      "matchPackageNames": [
        "jest*"
      ],
      "groupName": "jest"
    },
    {
      "matchPackageNames": [
        "moment*"
      ],
      "groupName": "moment"
    },
    {
      "matchPackageNames": [
        "pg*"
      ],
      "groupName": "pg"
    },
    {
      "matchPackageNames": [
        "pino*"
      ],
      "groupName": "pino"
    },
    {
      "matchPackageNames": [
        "postcss*"
      ],
      "groupName": "postcss"
    },
    {
      "matchPackageNames": [
        "redux*"
      ],
      "groupName": "redux"
    },
    {
      "matchPackageNames": [
        "rollup*"
      ],
      "groupName": "rollup"
    },
    {
      "matchPackageNames": [
        "serverless*"
      ],
      "groupName": "serverless"
    },
    {
      "matchPackageNames": [
        "@socket.io/**",
        "socket.io*"
      ],
      "groupName": "socket.io"
    },
    {
      "matchPackageNames": [
        "@testing-library/**"
      ],
      "groupName": "testing-library"
    },
    {
      "matchPackageNames": [
        "@types/**",
        "typescript"
      ],
      "groupName": "typescript"
    },
    {
      "matchPackageNames": [
        "@pmmmwh/react-refresh-webpack-plugin",
        "assets-webpack-plugin",
        "babel-loader",
        "css-loader",
        "css-minimizer-webpack-plugin",
        "exports-loader",
        "html-webpack-plugin",
        "loader-utils",
        "mini-css-extract-plugin",
        "null-loader",
        "style-loader",
        "terser-webpack-plugin",
        "warnings-to-errors-webpack-plugin",
        "webpack*"
      ],
      "groupName": "webpack"
    }
  ],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 5,
  "rangeStrategy": "bump",
  "rollbackPrs": true,
  "schedule": [
    "after 12:01 am on Friday on the 1st through 7th day of the month every 3 month"
  ],
  "timezone": "Australia/Melbourne",
  "vulnerabilityAlerts": {
    "enabled": true,
    "rangeStrategy": "bump"
  }
}
